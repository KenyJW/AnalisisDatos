        "from folium.plugins import MiniMap\n",
        "from branca.element import Figure\n",
        "\n",
        "popuptext = '<b>Heredia</b>'\n",
        "\n",
        "my_map = folium.Map(location=[9.970715418178191, -84.12912520723047], zoom_start = 16, tiles = 'stamenterrain')\n",
        "\n",
        "folium.Marker(location = [9.970715418178191, -84.12912520723047], popup = popuptext, icon = folium.Icon (color = \"orange\", icon=\"ok-sign\")).add_to(my_map)\n",
        "\n",
        "folium.Circle(location = [9.970715418178191, -84.12912520723047], color = \"purple\", weight=6,fill_color = \"red\", fill_opacity = 0.5, tooltip = \"Heredia\").add_to(my_map)\n",
        "\n",
        "minimap = MiniMap(tile_layer = 'stamenterrain', position = 'bottomright')\n",
        "\n",
        "my_map.add_child(minimap)\n",
        "my_map.save('C:folium.html')\n",
        "\n",
        "my_map"
